@inproceedings{Hromei2025GroundedSRL,
  author    = {Claudiu Daniel Hromei and Antonio Scaiella and Danilo Croce and Roberto Basili},
  title     = {Grounded Semantic Role Labelling from Synthetic Multimodal Data for Situated Robot Commands},
  booktitle = {Proceedings of the 2025 Conference on Empirical Methods in Natural Language Processing (EMNLP 2025)},
  year      = {2025},
  address   = {Suzhou, China},
  month     = nov,
  publisher = {Association for Computational Linguistics},
  abstract  = {We introduce a novel Multimodal Grounded Semantic Role Labelling (G-SRL) framework that combines frame semantics with perceptual grounding, enabling robots to interpret natural language commands via multimodal logical forms. Our automated data generation pipeline uses LLM-guided prompt engineering and diffusion models to synthesize over 11,000 image–command pairs for situated Human-Robot Interaction (HRI).},
  note      = {Oral presentation, Main Conference, Session 7, Thu. Nov 6, 10:30–12:00.},
  url       = {https://2025.emnlp.org/}
}

@inproceedings{DeLuca2025SanskritVoyager,
  author    = {Giacomo De Luca and Danilo Croce and Roberto Basili},
  title     = {Sanskrit Voyager: Unified Web Platform for Interactive Reading and Linguistic Analysis of Sanskrit Texts},
  booktitle = {Proceedings of the 2025 Conference on Empirical Methods in Natural Language Processing: System Demonstrations"},
  year      = {2025},
  address   = {Suzhou, China},
  month     = nov,
  publisher = {Association for Computational Linguistics},
  abstract  = {Sanskrit Voyager is a unified web platform for searching, reading, and linguistically analyzing Sanskrit texts. It provides morphological analysis, sandhi resolution, interactive reading, and corpus exploration across 900+ texts, achieving over 92\% parsing accuracy on complex compounds. The system is open-source and designed for both researchers and learners.},
  note      = {Demo session, Wed. Nov 5, 11:00–12:30, Hall C, Session 2.},
  url       = {https://2025.emnlp.org/}
}

