@inproceedings{Borazio2025UniTorBioASQ,
  author    = {Federico Borazio and Andriy Shcherbakov and Danilo Croce and Roberto Basili},
  title     = {UniTor at BioASQ 2025: Modular Biomedical QA with Synthetic Snippets and Multiple Task Answer Generation},
  booktitle = {Working Notes of CLEF 2025 - Conference and Labs of the Evaluation Forum},
  year      = {2025},
  editor    = {Guglielmo Faggioli and Nicola Ferro and Allan Hanbury and Evangelos Kanoulas},
  address   = {Madrid, Spain},
  month     = sep,
  publisher = {CEUR Workshop Proceedings},
  volume    = {4038},
  pages     = {165--177},
  url       = {https://ceur-ws.org/Vol-4038/paper_10.pdf},
  urn       = {urn:nbn:de:0074-4038-10},
  organization = {CLEF Association},
  abstract  = {We present UniTor@BioASQ, a retrieval-augmented pipeline for biomedical question answering, developed for the BioASQ 2025 Challenge (Task 13b). The system combines multi-stage document retrieval with LLM-guided snippet extraction and unified answer generation across diverse biomedical question types, leveraging synthetic LLM-generated snippets for semantic reranking. UniTor@BioASQ achieves robust performance across all BioASQ subtasks using compact open-source models.}
}

